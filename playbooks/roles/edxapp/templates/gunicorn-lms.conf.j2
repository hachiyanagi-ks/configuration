{% if ansible_processor|length > 0 %}
bind = '{{ edxapp_lms_gunicorn_host }}:{{ edxapp_lms_gunicorn_port }}'
workers = {{ ansible_processor|length * worker_core_mult.lms }} 
max_requests = 10000
preload_app = True
user = None
group = None
pythonpath = "{{ edxapp_code_dir }}"
timeout = 300
{% else %}
bind = '{{ edxapp_lms_gunicorn_host }}:{{ edxapp_lms_gunicorn_port }}'
workers = {{ worker_core_mult.lms }} 
max_requests = 10000
preload_app = True
user = None
group = None
pythonpath = "{{ edxapp_code_dir }}"
timeout = 300
{% endif %}
